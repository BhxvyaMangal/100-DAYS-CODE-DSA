//Problem: A system receives two separate logs of user arrival times from two different servers. Each log is already sorted in ascending order.Your task is to create a single chronological 
log that preserves the correct order of arrival
#include <stdio.h>

void mergeLogs(int log1[], int n1, int log2[], int n2, int result[]) {
    int i = 0, j = 0, k = 0;

    while(i < n1 && j < n2) {
        if(log1[i] <= log2[j]) {
            result[k++] = log1[i++];
        } else {
            result[k++] = log2[j++];
        }
    }

    while(i < n1) {
        result[k++] = log1[i++];
    }

    while(j < n2) {
        result[k++] = log2[j++];
    }
}
